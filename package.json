    "errorify": "^0.3.1",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.1",
    "pkgcloud": "^1.4.0",
    "semver": "^5.3.0",
    "teamcity-properties": "^1.1.0",
    "watch-glob": "^0.1.3",
    "yargs": "^10.0.3"
  }
}
