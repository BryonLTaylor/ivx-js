{
  "name": "ivx-js",
  "version": "0.8.0",
  "description": "An interactive experience src. ",
  "main": "index.js",
  "scripts": {
    "build": "node build.js --should-build",
    "setup": "node build.js --should-build --setup",
    "webpack-dev-server" : "node_modules/.bin/webpack-dev-server --open",
    "local-build": "node build.js --should-build --local --ui=$UI --data=$DATA --validation=$VALIDATION --analytics=$ANALYTICS",
    "start" : "node_modules/.bin/npm-run-all -p webpack-dev-server local-build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.2.0",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^2.30.1",
    "immutable": "^3.8.2",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.4",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.19.0",
    "supports-color": "^5.0.0",
    "webpack": "^3.8.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-hot-middleware": "^2.20.0"
  },
  "dependencies": {
