{
  "name": "ivx-js",
  "version": "1.0.0",
  "description": "An interactive experience library. ",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/npm-run-all \"build:create-build-folder\" build:compress:angular-ivx-js build:angular-ivx-js build:create-ivxjs-css build:compress:create-ivxjs-css",
    "demo:all:update-js" : "node_modules/.bin/npm-run-all \"demo:hello-world:update-js\" \"demo:iphone-inline:update-js\" \"demo:ivx-io:update-js\"  \"demo:set-up-a-404-state:update-js\"  \"demo:templates:update-js\" ",
    "demo:hello-world:update-js" : "node_modules/.bin/copyfiles -u 1 dist/*.js demos/hello-world/js/lib",
    "demo:iphone-inline:update-js" : "node_modules/.bin/copyfiles -u 1 dist/*.js demos/iphone-inline/js/lib",
    "demo:ivx-io:update-js" : "node_modules/.bin/copyfiles -u 1 dist/*.js demos/ivx-io/js/lib",
    "demo:set-up-a-404-state:update-js" : "node_modules/.bin/copyfiles -u 1 dist/*.js demos/set-up-a-404-state/js/lib",
    "demo:templates:update-js" : "node_modules/.bin/copyfiles -u 1 dist/*.js demos/templates/js/lib",
    "dev": "node_modules/.bin/npm-run-all -p \"dev:angular:watch-files\" \"dev:demo-ivx-io:watch-files\" ",
    "dev:start": "node_modules/.bin/npm-run-all -p \"dev\"  \"dev:demo:create-server\"",
    "dev:angular-ivx-js:watch-files": "node_modules/.bin/watchify library/angular/app.js -o dist/angular.ivx.js -t [ babelify --presets [ es2015 ] ] -v --debug",
    "dev:demo:copy-files": "node_modules/.bin/copyfiles -u 1 dist/*.js demos/core/js/lib",
    "dev:demo-core:compile-less": "node_modules/.bin/lessc \"less/ivxjs.less\" \"demos/core/css/ivxjs.css\" --source-map-map-inline",
    "dist": "node_modules/.bin/npm-run-all \"buid:dist:angular-ivx-js\" \"build:compress:angular-ivx-js\" ",
    "dist:angular-ivx-js": "node_modules/.bin/browserify library/angular/app.js -o dist/angular.ivx.js -t [ babelify --presets [ es2015 ] ] -v --debug",
    "dist:angular-ivx-js:compress": "node_modules/.bin/browserify -g uglifyify library/angular/app.js  -t [ babelify --presets [ es2015 ] ] > dist/angular.ivx.min.js",
    "dist:ivxjs-css": "node_modules/.bin/lessc \"less/ivxjs.less\" \"build/ivxjs.css\"",
    "dist:ivxjs-css:compress": "node_modules/.bin/lessc --clean-css \"less/ivxjs.less\" \"npm-package-files/ivx-js-angular/dist/ivxjs.min.css\"",
    "docs:create-server": "node_modules/.bin/browser-sync start --server docs/esdocs --files \"docs/esdocs/**/*.*\" --port 8890 --ui-port 8891",
    "docs:esdocs": "node_modules/.bin/esdoc -c docs/esdoc.json",
    "docs:edit": "node_modules/.bin/npm-run-all -p dev:create-docs dev:create-docs:create-server dev:create-docs:watch-files",
    "docs:developer:create-server" : "node_modules/.bin/browser-sync start --server docs/esdocs --files \"docs/esdocs/manual/**/*.*\" --port 8890 --ui-port 8891",
    "docs:developer:edit": "node_modules/.bin/npm-run-all -p docs:developer:watch docs:developer:update-sample-json \"docs:developer:create-server\"",
    "docs:developer:move-sample-json": "node_modules/.bin/copyfiles -f -u 1 docs/developer/sample-json/*.json docs/esdocs/manual/sample-json",
    "docs:developer:watch": "node_modules/.bin/watch \"node_modules/.bin/esdoc -c docs/esdoc-manuals.json\" \"docs/developer\"",
    "docs:developer:update-sample-json": "node_modules/.bin/watch \"npm run docs:developer:move-sample-json\" \"docs/developer/sample-json\"",
    "docs:watch-files": "node_modules/.bin/watch \"npm run dev:create-docs\" \"library\"",
    "publish:set-up": "node_modules/.bin/npm-run-all build",
    "sandbox:copy-deps": "node_modules/.bin/copyfiles -f dist/*.js deps/**/*.js sandbox/js/lib",
    "sandbox:copy-less": "node_modules/.bin/copyfiles -u 1 less/**/* sandbox",
    "sandbox:create-server": "node_modules/.bin/browser-sync start --server sandbox --files \"sandbox/**/*.*\" --port 8888 --ui-port 8889",
    "sandbox:create:make-dir": "node_modules/.bin/mkdirp \"sandbox/css\" \"sandbox/js/lib\" \"sandbox/data\"",
    "sandbox:start": "node_modules/.bin/npm-run-all -p dev:sandbox:watch-files dev:sandbox:watch-less dev:sandbox:create-server dev:angular:watch-files \"dev:sandbox:make-project-js\"",
    "sandbox:watch-files": "node_modules/.bin/watch \"npm run dev:sandbox:copy-deps\" \"dist/\"",
    "sandbox:create:copy-index": "node_modules/.bin/copyfiles -f deps/sandbox-init/index.html sandbox",
    "sandbox:create:copy-data": "node_modules/.bin/copyfiles -f deps/sandbox-init/sandbox-init.json sandbox/data",
    "sandbox:make-project-js": "node_modules/.bin/watchify sandbox/projects/index.js -o sandbox/js/projects.js  -t [ babelify --presets [ es2015 ] ] -v --debug",
    "sandbox:create": "node_modules/.bin/npm-run-all \"dev:sandbox:create:mkdirp\" \"dev:sandbox:copy-deps\" \"dev:sandbox:create:copy-index\" \"dev:sandbox:copy-less\" \"dev:sandbox:create:copy-data\"",
    "sandbox:watch-less": "node_modules/.bin/watch \"npm run dev:sandbox:compile-less\" \"sandbox/less\"",
    "sandbox:compile-less": "node_modules/.bin/lessc \"sandbox/less/project.less\" \"sandbox/css/project.css\" --source-map-map-inline",
    "start": "npm run start:dev",
    "story-player:start": "node_modules/.bin/npm-run-all -p  \"dev:story-player:create-server\" \"dev:story-player:watch-files\" \"dev:story-player:ivx-js:watch-files\" \"dev:story-player:watch-less\" ",
    "story-player:create-server": "node_modules/.bin/browser-sync start --server story-player/local-build --files \"story-player/**/*.*\" --port 8888 --ui-port 8889",
    "story-player:story-js:watch-file": "node_modules/.bin/watchify -g uglifyify story-player/dev/js/project/story.js -o story-player/local-build/js/story.js  -t [ babelify --presets [ es2015 ] ] -v --debug",
    "story-player:watch-files": "node_modules/.bin/watchify story-player/dev/js/app.js -o story-player/local-build/js/story-player.js  -t [ babelify --presets [ es2015 ] ] -v --debug",
    "story-player:watch-less": "node_modules/.bin/watch \"npm run dev:story-player:compile-less\" \"story-player/dev/less\"",
    "story-player:compile-less": "node_modules/.bin/lessc \"story-player/dev/less/styles.less\" \"story-player/local-build/css/styles.css\" --source-map-map-inline",
    "story-player:ivx-js:watch-files": "node_modules/.bin/watchify library/angular/app.js -o story-player/local-build/js/angular.ivx.js  -t [ babelify --presets [ es2015 ] ] -v --debug",
    "story-player:compress-angular-ivx-js": "node_modules/.bin/browserify -g uglifyify library/angular/app.js  -t [ babelify --presets [ es2015 ] ] > story-player/build/js/angular.ivx.min.js",
    "story-player:compress-story-player-js": "node_modules/.bin/browserify -g uglifyify story-player/dev/js/app.js  -t [ babelify --presets [ es2015 ] ] > story-player/build/js/story-player.min.js",
    "story-player:move-semantic-js": "node_modules/.bin/copyfiles -f -u 2 story-player/dev/js/deps/semantic.min.js story-player/build/js",
    "story-player:move-iphone-inline-video-js": "node_modules/.bin/copyfiles -f -u 2 story-player/dev/js/deps/iphone-inline-video.browser.js story-player/build/js",
    "story-player:move-semantic-css": "node_modules/.bin/copyfiles -f -u 2 story-player/dev/less/deps/semantic.min.css story-player/build/css",
    "story-player:move-favicon": "node_modules/.bin/copyfiles -f -u 2 story-player/build-assets/img/favicon.ico story-player/build/img",
    "story-player:move-semantic-css-themes": "node_modules/.bin/copyfiles -f -u 2 story-player/dev/less/deps/themes/default/assets/fonts/*.* story-player/build/css/themes/default/assets/fonts",
    "story-player:move-tv4": "node_modules/.bin/copyfiles -f -u 2 story-player/dev/js/deps/tv4.js  story-player/build/js",
    "story-player:move-html": "node_modules/.bin/copyfiles -f -u 2 story-player/dev/views/index.html story-player/build",
    "story-player:create-build-folder": "node_modules/.bin/mkdirp \"story-player/build/js\" \"story-player/build/css\" \"story-player/build/img\"",
    "story-player:clean-build-folder": "node_modules/.bin/rimraf story-player/build",
    "story-player": "node_modules/.bin/npm-run-all \"build:story-player:clean-build-folder\" \"build:story-player:create-build-folder\" \"build:story-player:compress-angular-ivx-js\" \"build:story-player:compress-story-player-js\" \"build:story-player:move-semantic-js\" \"build:story-player:move-semantic-css\" \"build:story-player:move-html\" \"build:story-player:move-semantic-css-themes\" \"build:story-player:move-iphone-inline-video-js\" \"build:story-player:move-tv4\" \"build:story-player:move-favicon\"",
    "test": "npm run test:start",
    "test:create-server": "node_modules/.bin/browser-sync start --server docs --files \"docs/**/*.*\" --startPath \"tests/karma-tests\" --port 8890 --ui-port 8891",
    "test:start": "node_modules/.bin/npm-run-all -p dev:test:create-server start:testskarma",
    "test:start:karma": "node_modules/.bin/karma start karma.conf.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "angular": "^1.5.0",
    "angular-sanitize": "^1.5.0",
    "babel": "^6.5.2",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "browserify-istanbul": "^2.0.0",
    "copyfiles": "^0.2.1",
    "esdoc": "^0.4.6",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.3",
    "jasmine-core": "^2.2.0",
    "karma": "*",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserify": "^5.0.3",
    "karma-chrome-launcher": "^0.1.7",
    "karma-coverage": "^0.5.5",
    "karma-coverage-es6": "^0.2.7",
    "karma-fixture": "^0.2.6",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^0.3.5",
    "karma-json-fixtures-preprocessor": "0.0.6",
    "karma-mocha-reporter": "^2.0.1",
    "less": "^2.6.1",
    "less-plugin-clean-css": "^1.5.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^1.7.0",
    "rimraf": "^2.5.2",
    "traceur": "0.0.108",
    "tv4": "^1.2.7",
    "uglifyify": "^3.0.1",
    "watch": "^0.17.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {}
}
