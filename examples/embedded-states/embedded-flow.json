            "header": {
                "html": "<h1 class=\"ui centered align header\"><div class=\"content\">Parent Video State<div class=\"sub header\">Play the video and various scenes will appear that you can interact with</div></div></h1>"
            },
            "cuePoints": [
                {
                    "timeAt": 5,
                    "endAt": 7,
                    "always": true,
                    "eventName": "iVXjs:state:go",
                    "args": {
                        "route": "parent-video-state.video-in-video"
                    }
                },
                {
                    "timeAt": 10,
                    "endAt": 12,
                    "always": true,
                    "eventName": "iVXjs:state:go",
                    "args": {
                        "route": "parent-video-state.quick-links-state"
                    }
                },
                {
                    "timeAt": 14,
                    "endAt": 16,
                    "always": true,
                    "eventName": "iVXjs:state:go",
                    "args": {
                        "route": "parent-video-state.get-info"
                    }
                }
            ],
            "onVideoReady": [
                {
                    "eventName": "animateElement",
                    "args": {
                        "element": ".video-state-container",
                        "animationClasses": "show"
                    }
                }
            ],
            "embeddedViews": [
                {
                    "id": "funnel-survey",
                    "name": "Funnel Survey",
                    "type": "custom",
                    "classes": "basic",
                    "appendTo": "#parent-video-state",
                    "states": [
                        {
                            "stateId": "get-info",
                            "next": [
                                {
                                    "route": "^.survey-thank-you"
                                }
                            ]
                        },
                        {
                            "stateId": "survey-thank-you",
                            "next": [
                                {
                                    "route": "^"
                                }
                            ]
                        }
                    ]
                },
                {
                    "id": "video-in-video",
                    "name": "Video on Video",
                    "type": "custom",
                    "classes": "basic",
                    "appendTo": "#parent-video-state",
                    "states": [
                        {
                            "stateId": "video-in-video",
                            "next": [
                                {
                                    "route": "^"
                                }
                            ]
                        }
                    ]
